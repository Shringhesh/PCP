
def findMedianSortedArrays(nums1, nums2):
        nums1.extend(nums2)
        nums1.sort()
        a=len(nums1)
        b=a//2
        if(a%2!=0):
            return float(nums1[b])
        else:
            return float((nums1[b]+nums1[b-1])/2)
nums1=[1,2]
nums2=[3,4]
median=findMedianSortedArrays(nums1,nums2)
print(median)
